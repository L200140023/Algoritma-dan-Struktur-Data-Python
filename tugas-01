def kalimat(string) :
    vowel = "abcdefghijklmnopqrstuvwxyz"
    vowel2= "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
    jumkecil =""
    jumbesar =""
    for character in string :
        if character in vowel :	
            jumkecil += character         
        elif character in vowel2 :	
            jumbesar += character
    print "Kecil= "+ str(len(jumkecil))
    print "Besar= "+ str(len(jumbesar))
    if len(jumkecil)>len(jumbesar):
        print 'Jumlah Huruf kecil lebih banyak'
    elif len(jumkecil)<len(jumbesar):
        print 'Jumlah Huruf Besar lebih banyak'
    else:
        print 'Jumlah keduanya sama'
